<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.flowplayer.player.demo">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".demos.FlowplayerSupportFragmentActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />
        <activity
                android:name=".demos.FlowplayerSupportFragmentXmlActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />

        <activity
                android:name=".demos.FlowplayerViewActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />
        <activity
                android:name=".demos.FlowplayerViewAppCompatActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />

        <meta-data
                android:name="com.flowplayer.accessToken"
                android:value="YOU_ACCESS_TOKEN"/>

    </application>
</manifest>